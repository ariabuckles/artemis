    "asap": "^2.0.3",
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "classnames": "^1.1.4",
    "draft-js": "^0.10.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "kas": "https://github.com/Khan/KAS.git",
    "katex": "^0.7.1",
    "kmath": "^0.0.1",
    "math-input": "git+https://github.com/ClassJourney/math-input.git#classjourney",
    "mathquill": "git+https://github.com/ClassJourney/mathquill.git",
    "prettier": "^1.1.0",
    "react": "^15.5.3",
    "react-addons-create-fragment": "^15.5.3",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-pure-render-mixin": "^15.5.2",
    "react-components": "^0.4.0",
    "react-dom": "^15.5.3",
    "simple-markdown": "^0.2.1"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "pretty": "make pretty",
    "postinstall": "BABEL_ENV=production babel src --out-dir lib --copy-files"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/lib/",
      "<rootDir>/lib/lib/"
    ]
  }
}
